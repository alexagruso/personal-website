{
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,

    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."]
}
